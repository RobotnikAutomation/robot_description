<launch>
  <arg name="model" default="rbsherkan_std" />
  <include file="$(find robot_description)/launch/rbsherkan_robot_state.launch">
    <arg name="model" value="$(arg model)" />
  </include>
  <node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" required="true" />
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find robot_description)/rviz/default.rviz" required="true" />
</launch>